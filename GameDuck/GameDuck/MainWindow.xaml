<Window x:Class="GameDuck.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GameDuck"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" KeyDown="Window_KeyDown" KeyUp="Window_KeyUp">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="95*"/>
            <ColumnDefinition Width="169*"/>
        </Grid.ColumnDefinitions>
        <Image x:Name="GridObject" HorizontalAlignment="Left" Height="100" Margin="0,319,0,0" VerticalAlignment="Top" Width="792" IsEnabled="False" Source="Resources/grid.jpg" Stretch="None" Grid.ColumnSpan="2"/>
        <Image x:Name="DuckObject" HorizontalAlignment="Left" Height="70" Margin="39,260,0,0" VerticalAlignment="Top" Width="70" Source="Resources/duck.png"/>


    </Grid>
</Window>
